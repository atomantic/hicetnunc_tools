{
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "globals": {},
    "parserOptions": {
      "ecmaFeatures": {
        "blockBindings": true,
        "experimentalObjectRestSpread": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "ignorePatterns": ["node_modules/"],
    "rules": {
      "camelcase": 0,
      "comma-spacing": 0,
      "quotes": [
        1,
        "single",
        { "avoidEscape": true, "allowTemplateLiterals": true }
      ],
      "new-cap": 0,
      "no-console": 0,
      "no-empty": 0,
      "no-extend-native": 0,
      "no-multi-spaces": 0,
      "no-undef": 2,
      "no-unused-vars": 1,
      "space-infix-ops": 0,
      "strict": 0
    }
  }
  